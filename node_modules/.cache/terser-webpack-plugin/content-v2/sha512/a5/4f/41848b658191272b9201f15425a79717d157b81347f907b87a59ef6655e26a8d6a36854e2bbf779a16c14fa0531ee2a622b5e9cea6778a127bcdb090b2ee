ndex) {\r\n        return this.lastPage < index ? this.currentPage : index;\r\n    }\r\n\r\n    checkCurrentNotFirstThenDecrementByOne() {\r\n        return 0 > this.currentPage - 1 ? 0 : this.currentPage - 1;\r\n    }\r\n\r\n    checkCurrentNotLastThenIncrementByOne() {\r\n        return this.lastPage < this.currentPage + 1 ? this.currentPage : this.currentPage + 1;\r\n    }\r\n\r\n    checkPage(relative, index) {\r\n\r\n    }\r\n}\r\n\r\nexport default Paginate;","export default __webpack_public_path__ + \"static/media/it1.596b8099.png\";","export default __webpack_public_path__ + \"static/media/it2.a69144b3.png\";","export default __webpack_public_path__ + \"static/media/it3.1b2ff5bd.png\";","export default __webpack_public_path__ + \"static/media/e1.9a36d2a6.jpg\";","export default __webpack_public_path__ + \"static/media/e2.8d9529bf.jpg\";","export default __webpack_public_path__ + \"static/media/e3.f290c464.png\";","import itcase_first from \"../../../../image/portfolio/it1.png\"\r\nimport itcase_second from \"../../../../image/portfolio/it2.png\"\r\nimport itcase_third from \"../../../../image/portfolio/it3.png\"\r\nimport e_first from \"../../../../image/portfolio/e1.jpg\"\r\nimport e_second from \"../../../../image/portfolio/e2.jpg\"\r\nimport e_third from \"../../../../image/portfolio/e3.png\"\r\n\r\nexport const DataStoreSites = [\r\n    {\r\n        id:1,\r\n        title: \"Фриланс биржа itcase.com \",\r\n        body: \"Онлайн платформа, представляющая собой удобный способ взаимодействия между работодателем и специалистом. Данный формат сотрудничества не требует личной встречи, а обговаривать все детали проекта и оплачивать проделанный труд просто даже тогда, когда стороны находятся в разных уголках планеты\",\r\n        imageFirst:`${itcase_first}`,\r\n        imageSecond:`${itcase_third}`,\r\n        imageThird:`${itcase_second}`,\r\n        link:\"https://itcase.com/ru\"\r\n    },\r\n    {\r\n        id:2,\r\n        title: \"Комерческий сайт e-otse